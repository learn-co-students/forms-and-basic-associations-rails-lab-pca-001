<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  Artist Name:
  <%= f.text_field :artist_name %><br>

  Genre
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>

  Notes
  <input id="song_notes_1" name="song[notes][]">
  <input id="song_notes_2" name="song[notes][]">
  <input id="song_notes_3" name="song[notes][]">

  <%= f.submit %>
<% end %>


